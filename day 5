import java.util.Scanner;
//give power and base and find answer
public class Day5codingchallenge {
    public static void main(String[] args) {
            Scanner scn=new Scanner(System.in);
    System.out.print("enter number:");
        int num=scn.nextInt();
        System.out.print("enter power:");
        int power=scn.nextInt();
        int i,fact=num;
        for(i=1;i<power;i++){
             fact=fact*num;
        }
        System.out.println("power of "+num+" is "+fact);
        }
}





import java.util.Scanner;
//find factors by given number 
public class Day5codingchallenge {
    public static void main(String[] args) {
            Scanner scn=new Scanner(System.in);
    System.out.print("enter number:");
        int num=scn.nextInt();
        int i=2;
        System.out.println("factors are:");
        while(i<=num){
             if(num%i==0){
                 num=num/i;
                 System.out.print(i+" ");
             }
             else {
                 i++;
             }
} 
}
}




